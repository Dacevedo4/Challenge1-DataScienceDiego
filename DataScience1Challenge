        "\n",
        "import matplotlib.pyplot as plt\n",
        "import pandas as pd\n",
        "\n",
        "\n",
        "df_pivot_ingreso_alt = (\n",
        "    top3_categorias\n",
        "    .pivot_table(index='Tienda', columns='Categor√≠a del Producto', values='Ingreso', aggfunc='sum')\n",
        ")\n",
        "\n",
        "\n",
        "orden_categorias = df_pivot_ingreso_alt.sum().sort_values(ascending=False).index\n",
        "df_pivot_ingreso_alt = df_pivot_ingreso_alt[orden_categorias]\n",
        "\n",
        "\n",
        "paleta_colores = {\n",
        "    'Electr√≥nicos': '#1f77b4',\n",
        "    'Electrodom√©sticos': '#ff7f0e',\n",
        "    'Muebles': '#2ca02c'\n",
        "}\n",
        "colores = [paleta_colores.get(c, 'gray') for c in df_pivot_ingreso_alt.columns]\n",
        "\n",
        "\n",
        "fig, ax = plt.subplots(figsize=(9, 4))\n",
        "df_pivot_ingreso_alt.plot(kind='bar', color=colores, ax=ax, width=0.8, edgecolor='none')\n",
        "\n",
        "\n",
        "ax.set_title('TOP 3 INGRESOS POR CATEGOR√çA DE PRODUCTO', fontsize=14, fontweight='bold', pad=15)\n",
        "ax.set_xlabel('')\n",
        "ax.set_ylabel('')\n",
        "ax.tick_params(axis='x', rotation=0)\n",
        "ax.grid(False)\n",
        "\n",
        "\n",
        "for spine in ['top', 'right', 'left']:\n",
        "    ax.spines[spine].set_visible(False)\n",
        "\n",
        "\n",
        "for container in ax.containers:\n",
        "    ax.bar_label(container,\n",
        "                 fmt=lambda v: f\"{v/1e6:.0f} M\",\n",
        "                 label_type='edge',\n",
        "                 fontsize=9,\n",
        "                 padding=2)\n",
        "\n",
        "\n",
        "ax.legend(title='Categor√≠a del Producto', bbox_to_anchor=(1.02, 1), loc='upper left')\n",
        "ax.set_ylim(0, df_pivot_ingreso_alt.max().max() * 1.15)\n",
        "plt.tight_layout()\n",
        "plt.show()\n"
