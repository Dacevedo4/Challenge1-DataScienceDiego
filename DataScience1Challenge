      "cell_type": "code",
      "source": [
        "import matplotlib.pyplot as plt\n",
        "\n",
        "\n",
        "def graficar_productos(df, titulo, color):\n",
        "    tiendas = df['Tienda'].unique()\n",
        "\n",
        "    fig, axes = plt.subplots(len(tiendas), 1, figsize=(8, 4*len(tiendas)))\n",
        "\n",
        "    if len(tiendas) == 1:\n",
        "        axes = [axes]\n",
        "\n",
        "    for ax, tienda in zip(axes, tiendas):\n",
        "        df_tienda = df[df['Tienda'] == tienda]\n",
        "        ax.barh(df_tienda['Producto'], df_tienda['Cantidad'], color=color)\n",
        "        ax.set_title(f\"{titulo} - {tienda}\", fontsize=12, fontweight='bold')\n",
        "        ax.set_xlabel(\"Cantidad vendida\")\n",
        "        ax.set_ylabel(\"Producto\")\n",
        "\n",
        "    plt.tight_layout()\n",
        "    plt.show()\n",
        "\n",
        "\n",
        "graficar_productos(top_productos, \"Producto m√°s vendido\", \"green\")\n",
        "\n",
        "\n",
        "graficar_productos(bottom_productos, \"Producto menos vendido\", \"red\")\n"
      ],
      "metadata": {
