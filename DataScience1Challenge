      "cell_type": "code",
      "source": [
        "\n",
        "\n",
        "import seaborn as sns\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "\n",
        "df_pivot_cantidad = (\n",
        "    top3_categorias\n",
        "    .pivot_table(\n",
        "        index='Tienda',\n",
        "        columns='Categor√≠a del Producto',\n",
        "        values='Cantidad',\n",
        "        aggfunc='sum',\n",
        "        fill_value=0\n",
        "    )\n",
        ")\n",
        "\n",
        "\n",
        "categorias_ordenadas = (\n",
        "    df_pivot_cantidad\n",
        "    .sum()\n",
        "    .sort_values(ascending=False)\n",
        "    .index\n",
        ")\n",
        "df_pivot_cantidad = df_pivot_cantidad[categorias_ordenadas]\n",
        "\n",
        "\n",
        "df_cantidad_reset = (\n",
        "    df_pivot_cantidad\n",
        "    .reset_index()\n",
        "    .melt(id_vars='Tienda', var_name='Categor√≠a del Producto', value_name='Cantidad')\n",
        ")\n",
        "\n",
        "\n",
        "paleta_categorias = {\n",
        "    'Electr√≥nicos': '#1f77b4',\n",
        "    'Electrodom√©sticos': '#ff7f0e',\n",
        "    'Muebles': '#2ca02c'\n",
        "}\n",
        "\n",
        "\n",
        "plt.figure(figsize=(9, 4))\n",
        "ax = sns.barplot(\n",
        "    data=df_cantidad_reset,\n",
        "    x='Tienda',\n",
        "    y='Cantidad',\n",
        "    hue='Categor√≠a del Producto',\n",
        "    palette=paleta_categorias\n",
        ")\n",
        "\n",
        "\n",
        "ax.set_title(\n",
        "    'TOP 3 VOLUMEN DE VENTAS POR CATEGOR√çA DE PRODUCTO',\n",
        "    fontsize=14,\n",
        "    fontweight='bold',\n",
        "    pad=15\n",
        ")\n",
        "ax.set_xlabel('')\n",
        "ax.set_ylabel('')\n",
        "ax.grid(False)\n",
        "sns.despine(top=True, right=True, left=True)\n",
        "\n",
        "\n",
        "for p in ax.patches:\n",
        "    valor = p.get_height()\n",
        "    if valor > 0:\n",
        "        ax.text(\n",
        "            p.get_x() + p.get_width() / 2,\n",
        "            valor + (df_cantidad_reset['Cantidad'].max() * 0.02),\n",
        "            f'{int(valor)}',\n",
        "            ha='center',\n",
        "            va='bottom',\n",
        "            fontsize=9\n",
        "        )\n",
        "\n",
        "\n",
        "ax.legend(title='Categor√≠a del Producto', bbox_to_anchor=(1.02, 1), loc='upper left')\n",
        "plt.ylim(0, df_cantidad_reset['Cantidad'].max() * 1.15)\n",
        "plt.tight_layout()\n",
        "plt.show()\n"
      ],
      "metadata": {
