        "\n",
        "df_categoria = (\n",
        "    df.groupby(['Tienda', 'Categor√≠a del Producto'])\n",
        "      .agg(\n",
        "          Cantidad=('Precio', 'count'),\n",
        "          Ingreso=('Precio', 'sum')\n",
        "      )\n",
        "      .reset_index()\n",
        "      .sort_values(['Tienda', 'Ingreso'], ascending=[True, False])\n",
        ")\n",
        "\n",
        "\n",
        "print(\"Resumen de ingresos y cantidades por categor√≠a y tienda:\")\n",
        "display(df_categoria.head(20))\n"
