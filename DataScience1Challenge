        "import matplotlib.pyplot as plt\n",
        "\n",
        "df_plot = df_ingresos.copy().sort_values(by='Ingreso', ascending=False)\n",
        "\n",
        "colores = []\n",
        "for tienda in df_plot.index:\n",
        "    if tienda == 'T1':\n",
        "        colores.append('green')\n",
        "    else:\n",
        "        colores.append('lightgray')\n",
        "\n",
        "plt.figure(figsize=(6, 4))\n",
        "ax = plt.gca()\n",
        "\n",
        "barras = ax.bar(\n",
        "    df_plot.index,\n",
        "    df_plot['Ingreso'],\n",
        "    color=colores\n",
        ")\n",
        "\n",
